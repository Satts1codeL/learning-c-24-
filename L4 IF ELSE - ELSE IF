#include <stdio.h>

int main() {
    int a;
    printf("Enter your age: ");
    scanf("%d", &a);
    
    if (a > 60) {
        printf("You can drive & you are a Sr. citizen");
    }
    else if (a >= 40) {
        printf("You can drive & you are an elder");
    }
    else if (a > 18 || a == 18) {
        printf("You can drive");
    }
    else {
        printf("You cannot drive");
    }
    
    return 0;
}